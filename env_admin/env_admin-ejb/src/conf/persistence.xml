    <class>edu.umss.devportal.entity.ProjectEntity</class>
    <class>edu.umss.devportal.entity.ProjectMembershipEntity</class>
    <class>edu.umss.devportal.entity.RolEntity</class>
    <class>edu.umss.devportal.entity.ToolByProject</class>
    <class>edu.umss.devportal.entity.ToolEntity</class>
